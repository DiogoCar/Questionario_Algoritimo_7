programa {
  funcao inicio() {
    
inteiro n1, n2, n3

escreva("Digite o valor de N1: ")
leia(n1)

escreva("Digite o valor de N2: ")
leia(n2)

escreva("Digite o valor de N3: ")
leia(n3)


se (n1 > n2 e n1 > n3)
escreva("Maior valor: " + n1)

se (n2 > n1 e n2 > n3)
escreva("Maior valor: " + n2)

se (n3 > n2 e n3 > n1)
escreva("Maior valor: " + n3)

  }
}
